{
    "name": "rowy-run",
    "options": {
        "allow-unauthenticated": true,
        "memory": "2Gi"
    },
    "hooks": {
        "prebuild": {
            "commands": [
                "npm install",
                "npm run build",
                "cd build/hooks && node prebuild"
            ]
        },
        "postcreate": {
            "commands": [
                "cd build/hooks && node postcreate"
            ]
        }
    }
}